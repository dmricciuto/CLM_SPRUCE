use all_hourly.nc
show data
plot fsds[l=3000:3100]
plot fsds[i=1,l=3000:3100]
plot fsds[i=1,l=4000:4100]
plot fsds[i=1,l=1:100]
plot tbot[i=1,l=1:100]
plot tbot[i=1,l=1:300]
plot tbot[i=1,l=4000:4200]
exit
